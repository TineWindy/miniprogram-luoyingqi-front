<scroll-view scroll-y class="scrollPage tasks">
  <view class="cu-card">
    <view class="cu-item bg-img shadow-blur" style="background-image:url({{backImgs[index%3]}})" bindtap="toChild" data-taskId="{{item.id}}" wx:for="{{taskList}}" wx:key wx:>
      <view class="cardTitle">
        {{item.taskName}}
      </view>
    </view>
  </view>
  <view class="cu-tabbar-height"></view>
</scroll-view>