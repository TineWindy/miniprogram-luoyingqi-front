/*页面弹性盒子布局*/
page{
  width: 100%;
  height: 85%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
/*按钮*/
.wenzi{
  position:absolute;
  left:50rpx;
  top: 70rpx;
  z-index:100;
  color:white;
  font-size: 0.9em;
}
.marginTopAndSomeCommon{
  background-color: #FDF8F6;
  width: 92%;
}
.marginTopAndSomeCommon::after{
  border: none;
}